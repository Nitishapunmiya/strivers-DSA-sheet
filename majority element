class Solution {
public:
    int majorityElement(vector<int>& v) {
        int cnt=0;
        int el;
        for(int i=0;i<v.size();i++){
            if (cnt==0){
                cnt++;
                el=v[i];
            }
            else if (v[i]==el){
                cnt++;
            }
            else{
                cnt--;
            }
        }
        return el;
        }
        
    };
